    <typeDescription>
      <name>edu.cmu.lti.wenyiwan.Trigram</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>TokenList</name>
          <description>lits of tokens in the trigram</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.lti.wenyiwan.Token</elementType>
        </featureDescription>
        <featureDescription>
          <name>begin</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>end</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
